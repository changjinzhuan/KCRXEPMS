<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置框内填充颜色 -->
    <solid android:color="#E91E63" />

    <!-- 设置边框宽度和颜色 -->
    <stroke
        android:width="1dip"
        android:color="#000000" />
    <!-- 设置圆角半径 -->
    <corners android:radius="3dp" />
    <!-- 设置边距 -->
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />
</shape>